         version="3.1">
    <filter>
        <filter-name>setEncoding</filter-name>
        <filter-class>util.EnCode_Filter</filter-class>
        <init-param>
            <param-name>name</param-name>
            <param-value>value</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>setEncoding</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
